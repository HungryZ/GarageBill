.header {
  margin: 0 auto;
  width: 95%;
  box-sizing: border-box;
  padding-top: 1.5%;
  height: 4%;
  display: flex;
  justify-content: space-between;
  /* align-items: center; */
}

.body {
  margin: 0 auto;
  width: 96%;
  /* height: 94%; */
  display: flex;
  flex-direction: column;
}

.paramater {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.border {
  border: 1px solid black;
  box-sizing: border-box;
}

.paramaterCell {
  width: 50%;
  padding-left: 20rpx;
  /* border-right: 1px solid black; */
}

.row {
  display: flex;
  justify-content: space-between;
}

.footer {
  margin-right: 40rpx;
  text-align: right;
}